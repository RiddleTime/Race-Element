<UserControl x:Class="ACCSetupApp.Controls.SetupRenderer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ACCSetupApp.Controls"
         
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <FlowDocumentScrollViewer Opacity="0.5" Focusable="False" Cursor="Arrow">
            <FlowDocument  x:Name="flowDocument" Focusable="False" Cursor="Arrow" Background="Black" RenderOptions.ClearTypeHint="Enabled" AllowDrop="False">
            </FlowDocument>
        </FlowDocumentScrollViewer>
    </Grid>
</UserControl>
